	<script type="text/javascript">
		$(function(){
			$('.selection-period-ul li').click(function(){
				$('.selection-period-ul li').removeClass('selected');
				$(this).siblings('li').find('.rate-period').fadeOut(100);
				$(this).addClass('selected');
				$(this).find('.rate-period').fadeIn(100);
			});
			$('.selection-period-ul li').hover(function(){
				$(this).find('.rate-period').fadeIn(100);
			},function(){
				if(!$(this).hasClass('selected')){
					$(this).find('.rate-period').fadeOut(100);
				}
			});
		})
	</script>
